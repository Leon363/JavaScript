
function getRandomNumber(min, max) {
    let p = Math.floor(Math.random() * (max - min) + min)
    if(min > max){
        console.log(Math.floor(Math.random() * (min - max) + max))
        return 0;
    }
    console.log(p)
}
getRandomNumber(1, 10)

